    {% elseif  is_granted("ROLE_ADMIN") %}
        <h1>
            Witaj Administratorze!
        </h1>
    {%  else %}
        <h1>
            Witaj Nieznajomy!
        </h1>
  {% endif %}


{% endblock %}
